{
  "name": "nextjs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "package:deps": "ncu",
    "package:update": "ncu -i",
    "package:update:force": "ncu -u",
    "eslint": "next lint",
    "eslint:fix": "next lint --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "tsc": "tsc --noEmit",
    "ci": "npm run prettier:fix && npm run eslint:fix && npm run tsc && npm run prisma:format && npm run docs",
    "docs": "zodocs",
    "prisma:format": "npx prisma format",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate",
    "prisma:seed": "npx prisma db seed",
    "prisma:fresh": "prisma migrate reset --force",
    "prisma:studio": "npx prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^6.7.0",
    "next": "15.3.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.15.3",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "lodash": "^4.17.21",
    "npm-check-updates": "^18.0.1",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "prisma": "^6.7.0",
    "sql-formatter": "^15.6.1",
    "tailwindcss": "^4.1.4",
    "tsx": "^4.19.4",
    "type-fest": "^4.40.1",
    "typescript": "^5.8.3",
    "zod": "^3.24.3",
    "zodocs": "file:../../sources"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
