{
  "name": "zodocs",
  "version": "1.0.0",
  "description": "A documentation generator for Zod schemas",
  "author": "nnst0knnt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nnst0knnt/zodocs"
  },
  "keywords": [
    "zod",
    "documentation",
    "typescript",
    "schema",
    "openapi"
  ],
  "main": "build/index.js",
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build",
    "bin"
  ],
  "scripts": {
    "build": "tsup",
    "package:deps": "ncu",
    "package:update": "ncu -i",
    "package:update:force": "ncu -u",
    "with-env": "dotenv -e .env",
    "format": "prettier --write \"./**/*.{ts,json,md}\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "tsc": "tsc --noEmit",
    "test": "vitest run",
    "ci": "npm run format && npm run lint:fix && npm run tsc && npm run build && npm run test"
  },
  "type": "module",
  "bin": {
    "zodocs": "./bin/zodocs.sh"
  },
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js",
      "require": "./build/index.js"
    }
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.0",
    "chalk": "^5.4.1",
    "lodash": "^4.17.21",
    "type-fest": "^4.39.1",
    "yaml": "^2.7.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@types/lodash": "4.17.16",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "dotenv-cli": "8.0.0",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "npm-check-updates": "17.1.18",
    "prettier": "3.5.3",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.29.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "3.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
